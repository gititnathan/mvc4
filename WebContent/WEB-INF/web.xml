<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<servlet>
		<servlet-name>front</servlet-name>
		<servlet-class>AppFrontController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>front</servlet-name>
		<url-pattern>*.app</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>member</servlet-name>
		<servlet-class>member.MemberServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>member</servlet-name>
		<url-pattern>*.member</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>board</servlet-name>
		<servlet-class>board.BoardServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>board</servlet-name>
		<url-pattern>*.board</url-pattern>
	</servlet-mapping>
		<servlet>
		<servlet-name>shop</servlet-name>
		<servlet-class>shop.ShopServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>shop</servlet-name>
		<url-pattern>*.shop</url-pattern>
	</servlet-mapping>
			<servlet>
		<servlet-name>mall</servlet-name>
		<servlet-class>shop.mall.MallServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>mall</servlet-name>
		<url-pattern>*.mall</url-pattern>
	</servlet-mapping>
			<servlet>
		<servlet-name>baseball</servlet-name>
		<servlet-class>baseball.BaseServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>baseball</servlet-name>
		<url-pattern>*.base</url-pattern>
	</servlet-mapping>
	<jsp-config>
		<taglib>
			<taglib-uri>http://www.springframework.org/tags/form</taglib-uri>
			<taglib-location>/WEB-INF/tlds/spring-form.tld</taglib-location>
		</taglib>
	</jsp-config>

</web-app>